#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/d/Level-02/azirVai/app/portfolio-auth-data/node_modules/ts-node/dist/node_modules:/proc/cygdrive/d/Level-02/azirVai/app/portfolio-auth-data/node_modules/ts-node/node_modules:/proc/cygdrive/d/Level-02/azirVai/app/portfolio-auth-data/node_modules:/proc/cygdrive/d/Level-02/azirVai/app/node_modules:/proc/cygdrive/d/Level-02/azirVai/node_modules:/proc/cygdrive/d/Level-02/node_modules:/proc/cygdrive/d/node_modules:/proc/cygdrive/d/Level-02/azirVai/app/portfolio-auth-data/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/d/Level-02/azirVai/app/portfolio-auth-data/node_modules/ts-node/dist/node_modules:/proc/cygdrive/d/Level-02/azirVai/app/portfolio-auth-data/node_modules/ts-node/node_modules:/proc/cygdrive/d/Level-02/azirVai/app/portfolio-auth-data/node_modules:/proc/cygdrive/d/Level-02/azirVai/app/node_modules:/proc/cygdrive/d/Level-02/azirVai/node_modules:/proc/cygdrive/d/Level-02/node_modules:/proc/cygdrive/d/node_modules:/proc/cygdrive/d/Level-02/azirVai/app/portfolio-auth-data/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-cwd.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin-cwd.js" "$@"
fi
